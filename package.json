{
  "name": "@devery/devery",
  "version": "0.1.58",
  "description": "Build decentralised apps on the Devery Protocol using Javascript",
  "main": "./dist/index.js",
  "scripts": {
    "start": "node --experimental-modules index.mjs",
    "debug": "nodemon --inspect server.js",
    "test": "truffle test test/*",
    "coverage": "cross-env NODE_ENV=test nyc truffle test && nyc report --reporter=text-lcov | coveralls",
    "generate-doc": "jsdoc --configure .jsdoc.json --verbose",
    "lint": "eslint devery/**/*.js test/**/*.js",
    "lint:fix": "eslint devery/**/*.js test/**/*.js --fix",
    "lint:watch": "esw devery/**/*.js test/**/*.js --watch",
    "inch": "inchjs suggest devery/"
  },
  "author": "Victor Eloy",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/devery/deveryjs"
  },
  "nyc": {
    "include": [
      "devery"
    ],
    "require": [
      "babel-register"
    ],
    "sourceMap": false,
    "instrument": false,
    "reporter": [
      "text",
      "lcov",
      "html"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.10.1",
    "@babel/core": "^7.10.2",
    "@babel/node": "^7.10.1",
    "@babel/preset-env": "^7.10.2",
    "@babel/register": "^7.10.1",
    "add": "^2.0.6",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-istanbul": "^6.0.0",
    "coveralls": "^3.0.2",
    "cross-env": "^5.1.4",
    "css-loader": "^0.26.1",
    "eslint": "^7.2.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-react": "^7.7.0",
    "eslint-plugin-react-native": "^3.2.1",
    "eslint-watch": "^3.1.4",
    "ink-docstrap": "^1.3.2",
    "jsdoc": "^3.5.5",
    "jsdoc-template-devery": "git+https://github.com/victorrseloy/jsdoc-template-devery.git",
    "json-loader": "^0.5.4",
    "mocha": "^5.1.1",
    "mocha-lcov-reporter": "^1.3.0",
    "npm": "^6.14.5",
    "nyc": "^11.7.1",
    "style-loader": "^0.13.1",
    "truffle-contract": "^1.1.11",
    "tui-jsdoc-template": "^1.2.2",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "yarn": "^1.22.4"
  },
  "dependencies": {
    "body-parser": "^1.18.2",
    "buffer": "^5.6.0",
    "cross-fetch": "^2.2.2",
    "crypto-js": "^3.1.9-1",
    "ethers": "^4.0.37",
    "inchjs": "^0.4.1",
    "truffle": "^4.1.14",
    "web3": "1.2.8"
  }
}
